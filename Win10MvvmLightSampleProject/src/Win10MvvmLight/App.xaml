<Application
    x:Class="Win10MvvmLight.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Win10MvvmLight"
	xmlns:vm="using:Win10MvvmLight.ViewModels"
	xmlns:convert="using:Win10MvvmLight.Converters"
    RequestedTheme="Light">
    <Application.Resources>
        <ResourceDictionary>
            <vm:WindowsViewModelLocator x:Key="ViewModelLocator" />
            <convert:ItemClickEventArgsConverter x:Key="ItemClickEventArgsConverter" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
